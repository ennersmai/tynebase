{
  "name": "tynebase-backend",
  "version": "1.0.0",
  "description": "TyneBase Backend API - Fastify server with RAG capabilities",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "dev:collab": "tsx watch src/collab-server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:worker": "node dist/worker.js",
    "start:collab": "node dist/collab-server.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "fastify",
    "api",
    "rag",
    "supabase"
  ],
  "author": "TyneBase Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.975.0",
    "@axiomhq/pino": "^1.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.1",
    "@google-cloud/vertexai": "^1.10.0",
    "@hocuspocus/server": "^3.4.3",
    "@supabase/supabase-js": "^2.39.3",
    "@tavily/core": "^0.7.1",
    "axios": "^1.13.2",
    "dotenv": "^16.4.1",
    "fastify": "^4.26.0",
    "mammoth": "^1.11.0",
    "pdf-parse": "^2.4.5",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "prosemirror-markdown": "^1.13.3",
    "prosemirror-model": "^1.25.4",
    "prosemirror-state": "^1.4.4",
    "tiktoken": "^1.0.22",
    "turndown": "^7.2.2",
    "y-prosemirror": "^1.3.7",
    "yjs": "^13.6.29",
    "yt-dlp-exec": "^1.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/turndown": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
